      <DTS:VariableValue
        DTS:DataType="8">SET NOCOUNT ON; 
DECLARE @OrgLevel AS INT = 2 
DECLARE @OrgUnit AS Table 
( 	
	[SourceKey] [varchar](255) , 	
	[Name] [varchar](255) , 	
	[DimOrganizationSourceKey] [int], 	
	[DimOrganizationUnitTypeLookup] VARCHAR(255), 	
	[IdentificationNumber] [varchar](20)  
); 
 
WITH base as
(
	SELECT 
	ou.*,
	
	CASE 
		WHEN (ou.OrganizationUnitTypeId IS NULL AND	ou.ParentOrganizationUnitId IS NULL) THEN 'PARENT ORGANIZATION'
		WHEN (ou.OrganizationUnitTypeId IS NOT NULL AND	ou.ParentOrganizationUnitId IS NOT NULL) THEN UPPER(LTRIM(RTRIM(ot.Description)))
		WHEN ( ou.OrganizationUnitTypeId IS NULL AND	ou.ParentOrganizationUnitId IS NOT NULL) THEN 'UNKNOWN'
	END AS OrgUnitName
FROM [dbo].[OrganizationUnit] as ou
LEFT JOIN dbo.OrganizationUnitType as ot on 
	ot.OrganizationUnitTypeId = ou.OrganizationUnitTypeId
),


cte AS 
(  	
	SELECT  		
		o.OrganizationId, 		
		ou.OrganizationUnitId, 		
		UnitName = COALESCE(ou.Name, o.name), 		
		IdentificationNumber, 	
		ou.OrganizationUnitTypeId,
		ou.OrgUnitName, 
		ParentOrganizationUnitId = ou.[OrganizationUnitId], 		
		1 AS OrgLevel 	
	FROM [dbo].Organization AS o 	
	INNER JOIN base AS ou ON 		
		ou.OrganizationId = o.OrganizationId 	
	WHERE ou.ParentOrganizationUnitId IS NULL 	
	
	UNION ALL 	
	SELECT  		
		st.OrganizationId, 		
		ou1.OrganizationUnitId, 		
		UnitName = ou1.Name, 			
		ou1.IdentificationNumber, 		
		ou1.OrganizationUnitTypeId, 
		ou1.OrgUnitName,
		ou1.ParentOrganizationUnitId, 		
		st.OrgLevel + 1 AS OrgLevel 	
	FROM base AS ou1 	
	INNER JOIN cte AS ST ON  		
		ou1.ParentOrganizationUnitId = ST.OrganizationUnitId 	
	WHERE 
		ou1.ParentOrganizationUnitId IS NOT NULL 
)  



INSERT INTO @OrgUnit 	( 		SourceKey, 		Name, 		DimOrganizationSourceKey, 		DimOrganizationUnitTypeLookup, 		IdentificationNumber 	)  	
SELECT 		
	SourceKey = cte.OrganizationUnitId, 		
	Name = CONVERT(VARCHAR(255),cte.UnitName), 		
	DimOrganizationSourceKey = cte.ParentOrganizationUnitId, 		
	DimOrganizationUnitTypeLookup = OrgUnitName, 		
	IdentificationNumber 	
FROM cte 	
WHERE  		
	cte.OrgLevel = @OrgLevel 	 		

SELECT  		
	ct.SYS_CHANGE_OPERATION, 
	SYS_CHANGE_VERSION = ct.as_of_change_version,
	paga_change_log_id = ct.row_id, SourceKey, 		
	base_query.Name, 		
	base_query.DimOrganizationSourceKey, 		
	base_query.DimOrganizationUnitTypeLookup, 		
	base_query.IdentificationNumber
FROM @OrgUnit AS base_query </DTS:VariableValue>
