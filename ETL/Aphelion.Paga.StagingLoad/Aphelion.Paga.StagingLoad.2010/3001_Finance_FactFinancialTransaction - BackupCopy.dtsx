                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @StartGroup AS INT = ?
DECLARE @EndGroup AS INT = ?

SELECT 
	 [SourceKey]
    ,[DimEffectiveDateID]
    ,[DimEffectiveTimeID]
    ,[DimFinancialTxDateID]
    ,[DimFinancialTxTimeID]
    ,[FinancialTxType_SourceKey]
    ,[FinancialTxSubType_SourceKey]
    ,[User_SourceKey]
    ,ProcessEvent_SourceKey
    ,[Reversed]
    ,[RelatedFinancialTx_SourceKey]
    ,[OriginalFinancialTx_SourceKey]
    ,[Currency_SourceKey]
    ,[TextDescription]
    ,[FinancialTx_Amount]
    ,[ExchangeRate]
    ,[ExternalReferenceNumber]
    ,[FinancialTx_Fee]
    ,[ForeignCurrencyAmount]
    ,[ReferenceNumber]
    ,[ShortCode]
    ,[Cancelled]
    ,[IsIntegrationTx]
    ,[FactIntegrationTx_SourceKey]
	,[BusinessType_SourceKey]
           
FROM 
(
	SELECT DISTINCT
		UserID ,
		[FinancialTransactionTypeId] 
	FROM ##FT_Picklist
	WHERE
		grp between @StartGroup and @EndGroup
) AS driver
CROSS APPLY
(
	SELECT
		SourceKey = ft.FinancialTransactionId, 
		DimEffectiveDateID = CONVERT(INT,CONVERT(VARCHAR(8),r2.EffDate,112)), 
		DimEffectiveTimeID =  CONVERT(INT,REPLACE(CONVERT(TIME(0),r2.EffDate),':','')),
		[DimFinancialTxDateID] = CONVERT(INT,CONVERT(VARCHAR(8),ft.TransactionDate,112)),
		[DimFinancialTxTimeID] =  CONVERT(INT,REPLACE(CONVERT(TIME(0),ft.TransactionDate),':','')),
		[FinancialTxType_SourceKey] = FinancialTransactionTypeId, 
		[FinancialTxSubType_SourceKey] = FinancialTransactionSubTypeId, 
		[User_SourceKey] = UserId, 
		ProcessEvent_SourceKey = COALESCE(ProcessId, -1), 
		CASE 
			WHEN FT.__ReversedBy IS NULL THEN 0
			ELSE 1
		END AS [Reversed],
		RelatedFinancialTx_SourceKey = r2.relatedTxID, 
		OriginalFinancialTx_SourceKey = r2.OrigTxID, 
		[Currency_SourceKey] = CurrencyId, 
		TextDescription = CONVERT(VARCHAR(100),[Description]), 
		FinancialTx_Amount = 
			CASE 
				WHEN (FT.Reverses IS NOT NULL OR FT.__Reverses IS NOT NULL) THEN -FT.Amount 
				ELSE FT.Amount 
			END, 
		ExchangeRate, 
		ExternalReferenceNumber,
		FinancialTx_Fee = Fee, 
		ForeignCurrencyAmount, 
		ReferenceNumber, 
		ShortCode,
		CASE 
			WHEN (FT.ReversedBy IS NOT NULL AND FT.Reverses IS NOT NULL) THEN 1
			WHEN (FT.ReversedBy IS NOT NULL AND FT.Reverses IS NULL) THEN 1
			WHEN (FT.ReversedBy IS NULL AND FT.Reverses IS NOT NULL) THEN 1
			WHEN (FT.ReversedBy IS NULL AND FT.Reverses IS NULL) THEN 0
		END  AS [Cancelled],
		CASE 
			WHEN it.FactIntegrationTx_SourceKey IS NULL THEN 0
			ELSE 1
		END AS [IsIntegrationTx],
		it.FactIntegrationTx_SourceKey,
		BusinessType_SourceKey = COALESCE(b.BusinessTypeId, 'UNKNOWN')
	FROM dbo.financialTransaction AS ft
	CROSS APPLY 
	(
		SELECT 
			r1.OrigTxID,
			r1.RelatedTxID,
			r1.EffDate
		FROM ##ReversalChain AS R1 
		WHERE 
			R1.TxID = ft.FinancialTransactionId
	) AS R2
	OUTER APPLY
	(
		SELECT
			FactIntegrationTx_SourceKey = intTx.IntegrationTransactionId
		FROM dbo.IntegrationTransaction as intTx
		WHERE 
			intTx.FinancialTransactionId = ft.FinancialTransactionId
			AND inttx.ExternalReferenceNumber = ft.ReferenceNumber
	) AS it
	OUTER APPLY
	(
		SELECT 
			TOP 1 O.BusinessTypeId
		FROM FinancialTransactionItem FTI 
		INNER JOIN FinancialAccount FA ON FTI.FinancialAccountId = FA.FinancialAccountId 
		INNER JOIN PagaAccount PA ON FA.AccountHolderId = PA.AccountHolderId
		INNER JOIN Organization O ON PA.PagaAccountId = O.PagaAccountId
		WHERE
			fti.FinancialTransactionId = ft.FinancialTransactionId

	) AS b
	WHERE 
		COALESCE(ft.userID,-1) = driver.UserID
		AND ft.FinancialTransactionTypeId = driver.FinancialTransactionTypeId
) base_query;</property>
