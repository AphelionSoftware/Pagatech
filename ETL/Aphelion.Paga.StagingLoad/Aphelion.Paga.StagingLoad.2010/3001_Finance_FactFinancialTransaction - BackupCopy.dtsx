    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{1A157695-4EAD-45CD-A016-0A457C30CC44}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT&#xA;&#x9;DENSE_RANK() OVER (ORDER BY pl.userid) AS grp,&#xA;&#x9;pl.UserID,&#xA;&#x9;pl.FinancialTransactionTypeId&#xA;INTO ##FT_PickList&#xA;FROM &#xA;(&#xA;&#x9;SELECT DISTINCT &#xA;&#x9;&#x9;&#x9;UserID = COALESCE(userID,-1), &#xA;&#x9;&#x9;&#x9;[FinancialTransactionTypeId] &#xA;&#x9;FROM &#xA;&#x9;(&#xA;&#x9;&#x9;SELECT&#x9;&#xA;&#x9;&#x9;&#x9;UserID,&#xA;&#x9;&#x9;&#x9;[FinancialTransactionTypeId],&#xA;&#x9;&#x9;&#x9;SourceKey =FinancialTransactionId&#xA;&#x9;&#x9;FROM dbo.financialTransaction&#xA;&#x9;) AS base_query&#xA;&quot; + @[$Package::EndQuery] +&quot;&#xA;) AS pl&#xA;&#xA;CREATE UNIQUE CLUSTERED INDEX ix_PickList ON  ##FT_PickList&#xA;(&#xA;&#x9;grp, UserID, FinancialTransactionTypeId&#xA;);&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="CreatePickListQuery">
      <DTS:VariableValue
        DTS:DataType="8">SELECT
	DENSE_RANK() OVER (ORDER BY pl.userid) AS grp,
	pl.UserID,
	pl.FinancialTransactionTypeId
INTO ##FT_PickList
FROM 
(
	SELECT DISTINCT 
			UserID = COALESCE(userID,-1), 
			[FinancialTransactionTypeId] 
	FROM 
	(
		SELECT	
			UserID,
			[FinancialTransactionTypeId],
			SourceKey =FinancialTransactionId
		FROM dbo.financialTransaction
	) AS base_query

) AS pl

CREATE UNIQUE CLUSTERED INDEX ix_PickList ON  ##FT_PickList
(
	grp, UserID, FinancialTransactionTypeId
);</DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{BC137B6B-08BD-4CAA-9ECA-0CB4B69DD9A8}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;CREATE TABLE #Reversals &#xA;(&#xA;&#x9;FinancialTransactionId INT,&#xA;&#x9;EffDate DATETIME,&#xA;&#x9;OrigTxID INT,&#xA;&#x9;RelatedTxID INT&#xA;);&#xA;&#xA;/*create table used to drive the population of the staging data*/&#xA;INSERT INTO #Reversals&#xA;(&#xA;&#x9;FinancialTransactionId,&#xA;&#x9;EffDate,&#xA;&#x9;OrigTxID,&#xA;&#x9;RelatedTxID&#xA;)&#xA;SELECT &#xA;&#x9;base_query.FinancialTransactionId,&#xA;&#x9;base_query.EffDate,&#xA;&#x9;base_query.OrigTxID,&#xA;&#x9;base_query.RelatedTxID&#xA;FROM&#xA;(&#xA;&#x9;SELECT&#xA;&#x9;&#x9;&#x9;ft1.FinancialTransactionId,&#xA;&#x9;&#x9;&#x9;ft1.TransactionDate AS EffDate,&#xA;&#x9;&#x9;&#x9;ft1.FinancialTransactionId AS OrigTxID,&#xA;&#x9;&#x9;&#x9;ft1.__Reverses AS RelatedTxID,&#xA;&#x9;&#x9;&#x9;SourceKey = ft1.FinancialTransactionId&#xA;&#x9;FROM dbo.FinancialTransaction AS ft1&#xA;) AS base_query&#xA;&quot; + @[$Package::EndQuery] +&quot;&#xA;&#xA;&#xA;CREATE UNIQUE CLUSTERED INDEX ix_Reversals_TxID ON #Reversals&#xA;(FinancialTransactionId, RelatedTxID);&#xA;&#xA;&#xA;CREATE NONCLUSTERED INDEX ix_Reversals_NullRelatedTxID ON #Reversals&#xA;(RelatedTxID)&#xA;WHERE RelatedTxID IS NULL;&#xA;&#xA;&#xA;CREATE NONCLUSTERED INDEX ix_Reversals_RelatedTxID ON #Reversals&#xA;(RelatedTxID);&#xA;&#xA;&#xA;/*Run recursive query to populate related financial transaction ID&#xA;for filter set created in previous step. */&#xA;&#xA;CREATE TABLE ##ReversalChain&#xA;(&#xA;&#x9;TxID  INT,&#xA;&#x9;EffDate  DATETIME,&#xA;&#x9;OrigTxID  INT,&#xA;&#x9;RelatedTxID  INT&#xA;);&#xA;&#xA;CREATE UNIQUE CLUSTERED INDEX ix_Reversals_TxID ON ##ReversalChain&#xA;(TxID);&#xA;&#xA;WITH reversals AS&#xA;(&#xA;&#xA;&#x9;SELECT&#xA;&#x9;&#x9;ft1.FinancialTransactionId,&#xA;&#x9;&#x9;ft1.EffDate AS EffDate,&#xA;&#x9;&#x9;ft1.FinancialTransactionId AS OrigTxID,&#xA;&#x9;&#x9;ft1.RelatedTxID AS RelatedTxID&#xA;&#x9;FROM #Reversals AS ft1&#xA;&#x9;WHERE&#xA;&#x9;&#x9;ft1.RelatedTxID IS NULL&#xA;&#x9;UNION ALL&#xA;&#x9;SELECT&#xA;&#x9;&#x9;ft2.FinancialTransactionId,&#xA;&#x9;&#x9;R.EffDate,&#xA;&#x9;&#x9;R.OrigTxID,&#xA;&#x9;&#x9;ft2.RelatedTxID&#xA;&#x9;FROM #Reversals AS ft2&#xA;&#x9;INNER JOIN reversals AS r&#xA;&#x9;&#x9;ON ft2.RelatedTxID = R.FinancialTransactionId&#xA;)&#xA;INSERT INTO ##ReversalChain&#xA;(&#xA;&#x9;TxID,&#xA;&#x9;EffDate,&#xA;&#x9;OrigTxID,&#xA;&#x9;RelatedTxID&#xA;)&#xA;SELECT&#xA;&#x9;reversals.financialTransactionId,&#xA;&#x9;reversals.EffDate,&#xA;&#x9;reversals.OrigTxID,&#xA;&#x9;reversals.relatedTxID&#xA;FROM reversals;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="CreateReversalsQuery">
      <DTS:VariableValue
        DTS:DataType="8">CREATE TABLE #Reversals 
(
	FinancialTransactionId INT,
	EffDate DATETIME,
	OrigTxID INT,
	RelatedTxID INT
);

/*create table used to drive the population of the staging data*/
INSERT INTO #Reversals
(
	FinancialTransactionId,
	EffDate,
	OrigTxID,
	RelatedTxID
)
SELECT 
	base_query.FinancialTransactionId,
	base_query.EffDate,
	base_query.OrigTxID,
	base_query.RelatedTxID
FROM
(
	SELECT
			ft1.FinancialTransactionId,
			ft1.TransactionDate AS EffDate,
			ft1.FinancialTransactionId AS OrigTxID,
			ft1.__Reverses AS RelatedTxID,
			SourceKey = ft1.FinancialTransactionId
	FROM dbo.FinancialTransaction AS ft1
) AS base_query



CREATE UNIQUE CLUSTERED INDEX ix_Reversals_TxID ON #Reversals
(FinancialTransactionId, RelatedTxID);


CREATE NONCLUSTERED INDEX ix_Reversals_NullRelatedTxID ON #Reversals
(RelatedTxID)
WHERE RelatedTxID IS NULL;


CREATE NONCLUSTERED INDEX ix_Reversals_RelatedTxID ON #Reversals
(RelatedTxID);


/*Run recursive query to populate related financial transaction ID
for filter set created in previous step. */

CREATE TABLE ##ReversalChain
(
	TxID  INT,
	EffDate  DATETIME,
	OrigTxID  INT,
	RelatedTxID  INT
);

CREATE UNIQUE CLUSTERED INDEX ix_Reversals_TxID ON ##ReversalChain
(TxID);

WITH reversals AS
(

	SELECT
		ft1.FinancialTransactionId,
		ft1.EffDate AS EffDate,
		ft1.FinancialTransactionId AS OrigTxID,
		ft1.RelatedTxID AS RelatedTxID
	FROM #Reversals AS ft1
	WHERE
		ft1.RelatedTxID IS NULL
	UNION ALL
	SELECT
		ft2.FinancialTransactionId,
		R.EffDate,
		R.OrigTxID,
		ft2.RelatedTxID
	FROM #Reversals AS ft2
	INNER JOIN reversals AS r
		ON ft2.RelatedTxID = R.FinancialTransactionId
)
INSERT INTO ##ReversalChain
(
	TxID,
	EffDate,
	OrigTxID,
	RelatedTxID
)
SELECT
	reversals.financialTransactionId,
	reversals.EffDate,
	reversals.OrigTxID,
	reversals.relatedTxID
FROM reversals;</DTS:VariableValue>
