	DimCurrencySourceKey, 
	TextDescription, 
	Amount, 
	ExchangeRate, 
	ExternalReferenceNumber,
	Fee, 
	ForeignCurrencyAmount, 
	ReferenceNumber, 
	ShortCode
FROM
(
	SELECT
		SourceKey = ft.FinancialTransactionId, 
