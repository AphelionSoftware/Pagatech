      <DTS:VariableValue DTS:DataType="8">SELECT 
		
	SourceKey = COALESCE(base_query.SourceKey,change_log.change_log_SourceKey),
	base_query.SystemDescription,
	base_query.Name, 
	base_query.TextDescription,
	change_operation = COALESCE(CONVERT(CHAR(1),change_log.change_operation),'I')
	
FROM 
	(
		SELECT 
			SourceKey = fa.roleId,
			SystemDescription = UPPER(CONVERT(VARCHAR(255),fa.[Namespace])),
			Name = CONVERT(VARCHAR(255), fa.Name),
			TextDescription = CONVERT(VARCHAR(1000), fa.Description)
		FROM dbo.[Role] AS fa

	) AS base_query</DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable DTS:CreationName="" DTS:DTSID="{BA090B64-310E-475E-BE3F-697592B4AE51}" DTS:EvaluateAsExpression="True" DTS:Expression="@[User::BaseQuery]+ &quot; &quot; +@[$Package::strParm1] +&quot;;&quot;" DTS:IncludeInDebugDump="2345" DTS:Namespace="User" DTS:ObjectName="FullQueryText">
      <DTS:VariableValue DTS:DataType="8">SELECT 
		
	SourceKey = COALESCE(base_query.SourceKey,change_log.change_log_SourceKey),
	base_query.SystemDescription,
	base_query.Name, 
	base_query.TextDescription,
	change_operation = COALESCE(CONVERT(CHAR(1),change_log.change_operation),'I')
	
FROM 
	(
		SELECT 
			SourceKey = fa.roleId,
			SystemDescription = UPPER(CONVERT(VARCHAR(255),fa.[Namespace])),
			Name = CONVERT(VARCHAR(255), fa.Name),
			TextDescription = CONVERT(VARCHAR(1000), fa.Description)
		FROM dbo.[Role] AS fa

	) AS base_query LEFT JOIN
																		(
