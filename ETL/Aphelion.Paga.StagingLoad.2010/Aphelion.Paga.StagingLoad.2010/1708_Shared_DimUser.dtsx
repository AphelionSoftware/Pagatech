    <DTS:Variable DTS:CreationName="" DTS:DTSID="{6B313448-609E-4364-AF97-7C14A190F355}" DTS:IncludeInDebugDump="6789" DTS:Namespace="User" DTS:ObjectName="CurrentVersion">
      <DTS:VariableValue DTS:DataType="3">0</DTS:VariableValue>
