    <DTS:Variable DTS:CreationName="" DTS:DTSID="{3826CC94-8D7D-41D2-84FE-D8B147C4D985}" DTS:IncludeInDebugDump="6789" DTS:Namespace="User" DTS:ObjectName="LastSyncVersion">
      <DTS:VariableValue DTS:DataType="3">6</DTS:VariableValue>
