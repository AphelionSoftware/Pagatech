    <DTS:PackageParameter DTS:CreationName="" DTS:DataType="8" DTS:Description="" DTS:DTSID="{5B83D189-9CBB-4405-BBE1-EDC79246C939}" DTS:ObjectName="strParm1" DTS:Required="0" DTS:Sensitive="0">
