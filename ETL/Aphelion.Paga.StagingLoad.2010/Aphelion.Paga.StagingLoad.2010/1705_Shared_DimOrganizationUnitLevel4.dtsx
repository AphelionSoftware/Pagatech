    <DTS:Variable DTS:CreationName="" DTS:DTSID="{1D874A9A-9089-406E-A0F7-BCE6F8C41EDD}" DTS:IncludeInDebugDump="6789" DTS:Namespace="User" DTS:ObjectName="CurrentVersion">
