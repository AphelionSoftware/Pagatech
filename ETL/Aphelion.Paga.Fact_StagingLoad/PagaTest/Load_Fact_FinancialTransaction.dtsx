      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C5DDBAEC-11AE-4593-A02E-00FF4F94B6E5}"
          SQLTask:SqlStatementSource="/* 20141012 Get Latest row moved to target */&#xA;DECLARE @MaxSourceKey AS INT = ?&#xA;&#xA;CREATE TABLE #Reversals &#xA;(&#xA;&#x9;FinancialTransactionId INT,&#xA;&#x9;EffDate DATETIME,&#xA;&#x9;OrigTxID INT,&#xA;&#x9;RelatedTxID INT&#xA;);&#xA;&#xA;/*create table used to drive the population of the staging data*/&#xA;INSERT INTO #Reversals&#xA;(&#xA;&#x9;FinancialTransactionId,&#xA;&#x9;EffDate,&#xA;&#x9;OrigTxID,&#xA;&#x9;RelatedTxID&#xA;)&#xA;SELECT&#xA;&#x9;&#x9;ft1.FinancialTransactionId,&#xA;&#x9;&#x9;ft1.TransactionDate AS EffDate,&#xA;&#x9;&#x9;ft1.FinancialTransactionId AS OrigTxID,&#xA;&#x9;&#x9;ft1.Reverses AS RelatedTxID&#xA;FROM dbo.FinancialTransaction AS ft1&#xA;WHERE&#xA;&#x9;ft1.FinancialTransactionId &gt; @maxSourceKey&#xA;&#xA;&#xA;INSERT INTO #Reversals&#xA;(&#xA;&#x9;financialTransactionID,&#xA;&#x9;EffDate,&#xA;&#x9;OrigTxID,&#xA;&#x9;RelatedTxID&#xA;)&#xA;SELECT&#xA;&#x9;&#x9;ft2.FinancialTransactionId,&#xA;&#x9;&#x9;ft2.TransactionDate AS EffDate,&#xA;&#x9;&#x9;ft2.FinancialTransactionId AS OrigTxID,&#xA;&#x9;&#x9;ft2.Reverses AS RelatedTxID&#xA;FROM dbo.FinancialTransaction AS ft2&#xA;WHERE &#xA;&#x9;ft2.FinancialTransactionId IN&#xA;&#x9;(&#xA;&#x9;&#x9;SELECT r.relatedtxID &#xA;&#x9;&#x9;FROM #Reversals as r&#xA;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;RelatedTxID IS NOT NULL &#xA;&#x9;&#x9;&#x9;AND RelatedTxID &lt;= @Maxsourcekey&#xA;&#x9;);&#xA;&#xA;CREATE UNIQUE CLUSTERED INDEX ix_Reversals_TxID ON #Reversals&#xA;(FinancialTransactionId, RelatedTxID);&#xA;&#xA;&#xA;CREATE NONCLUSTERED INDEX ix_Reversals_NullRelatedTxID ON #Reversals&#xA;(RelatedTxID)&#xA;WHERE RelatedTxID IS NULL;&#xA;&#xA;&#xA;CREATE NONCLUSTERED INDEX ix_Reversals_RelatedTxID ON #Reversals&#xA;(RelatedTxID);&#xA;&#xA;&#xA;/*Run recursive query to populate related financial transaction ID&#xA;for filter set created in previous step. */&#xA;&#xA;CREATE TABLE #ReversalChain&#xA;(&#xA;&#x9;TxID  INT,&#xA;&#x9;EffDate  DATETIME,&#xA;&#x9;OrigTxID  INT,&#xA;&#x9;RelatedTxID  INT&#xA;);&#xA;WITH reversals AS&#xA;(&#xA;&#xA;&#x9;SELECT&#xA;&#x9;&#x9;ft1.FinancialTransactionId,&#xA;&#x9;&#x9;ft1.EffDate AS EffDate,&#xA;&#x9;&#x9;ft1.FinancialTransactionId AS OrigTxID,&#xA;&#x9;&#x9;ft1.RelatedTxID AS RelatedTxID&#xA;&#x9;FROM #Reversals AS ft1&#xA;&#x9;WHERE&#xA;&#x9;&#x9;ft1.RelatedTxID IS NULL&#xA;&#x9;UNION ALL&#xA;&#x9;SELECT&#xA;&#x9;&#x9;ft2.FinancialTransactionId,&#xA;&#x9;&#x9;R.EffDate,&#xA;&#x9;&#x9;R.OrigTxID,&#xA;&#x9;&#x9;ft2.RelatedTxID&#xA;&#x9;FROM #Reversals AS ft2&#xA;&#x9;INNER JOIN reversals AS r&#xA;&#x9;&#x9;ON ft2.RelatedTxID = R.FinancialTransactionId&#xA;)&#xA;INSERT INTO #ReversalChain&#xA;(&#xA;&#x9;TxID,&#xA;&#x9;EffDate,&#xA;&#x9;OrigTxID,&#xA;&#x9;RelatedTxID&#xA;)&#xA;SELECT&#xA;&#x9;reversals.financialTransactionId,&#xA;&#x9;reversals.EffDate,&#xA;&#x9;reversals.OrigTxID,&#xA;&#x9;reversals.relatedTxID&#xA;FROM reversals;&#xA;&#xA;CREATE UNIQUE CLUSTERED INDEX ix_Reversals_TxID ON #ReversalChain&#xA;(TxID);&#xA;&#xA;/*populate temporary staging table used to by SSIS*/&#xA;&#xA;SELECT &#xA;&#x9;By_User.*,&#xA;&#x9;change_operation = 'I'&#xA;INTO ##BaseQuery&#xA;FROM &#xA;(&#xA;&#x9;SELECT DISTINCT &#xA;&#x9;&#x9;UserID = COALESCE(userID,-1), &#xA;&#x9;&#x9;[FinancialTransactionTypeId] &#xA;&#x9;FROM dbo.financialTransaction&#xA;&#x9;WHERE&#xA;&#x9;&#x9;FinancialTransactionId &gt; @MaxSourceKey /*same filter used to create reversals table, so we're limiting to the same set of TXIDs*/&#xA;) AS driver&#xA;CROSS APPLY&#xA;(&#xA;&#x9;SELECT&#xA;&#x9;&#x9;SourceKey = ft.FinancialTransactionId, &#xA;&#x9;&#x9;DimCreatedDateID = CONVERT(INT,CONVERT(VARCHAR(8),CreatedDate,112)), &#xA;&#x9;&#x9;DimCreatedTimeID = CONVERT(INT,REPLACE(CONVERT(VARCHAR(8), CreatedDate, 114),':','')), &#xA;&#x9;&#x9;DimEffectiveDateID = CONVERT(INT,CONVERT(VARCHAR(8),r2.EffDate,112)), &#xA;&#x9;&#x9;DimEffectiveTimeID = CONVERT(INT,REPLACE(CONVERT(VARCHAR(8), r2.EffDate, 114),':','')), &#xA;&#x9;&#x9;DimTransactionDateID = CONVERT(INT,CONVERT(VARCHAR(8),ft.TransactionDate,112)),&#xA;&#x9;&#x9;DimTransactionTimeID = CONVERT(INT,REPLACE(CONVERT(VARCHAR(8), ft.TransactionDate, 114),':','')), &#xA;&#x9;&#x9;DimFinancialTxTypeSourceKey = FinancialTransactionTypeId, &#xA;&#x9;&#x9;DimFinancialTxSubTypeSourceKey = FinancialTransactionSubTypeId, &#xA;&#x9;&#x9;DimOrganizationUnitLevel4SourceKey = orgUnit.OrganizationUnitId, &#xA;&#x9;&#x9;DimUserSourceKey = UserId, &#xA;&#x9;&#x9;FactProcessTxSourceKey = COALESCE(ProcessId, -1), &#xA;&#x9;&#x9;RelatedFactFinancialTxHeaderSourceKey = r2.relatedTxID, &#xA;&#x9;&#x9;OriginalFactFinancialTxHeaderSourceKey = r2.OrigTxID, &#xA;&#x9;&#x9;DimCurrencySourceKey = CurrencyId, &#xA;&#x9;&#x9;TextDescription = CONVERT(VARCHAR(100),[Description]), &#xA;&#x9;&#x9;Amount, &#xA;&#x9;&#x9;ExchangeRate, &#xA;&#x9;&#x9;ExternalReferenceNumber,&#xA;&#x9;&#x9;Fee, &#xA;&#x9;&#x9;ForeignCurrencyAmount, &#xA;&#x9;&#x9;ReferenceNumber, &#xA;&#x9;&#x9;ShortCode&#xA;&#x9;FROM dbo.financialTransaction AS ft&#xA;&#x9;CROSS APPLY &#xA;&#x9;(&#xA;&#x9;&#x9;SELECT &#xA;&#x9;&#x9;&#x9;r1.OrigTxID,&#xA;&#x9;&#x9;&#x9;COALESCE(r1.RelatedTxID,r1.OrigTxID) AS RelatedTxID,&#xA;&#x9;&#x9;&#x9;r1.EffDate&#xA;&#x9;&#x9;FROM #ReversalChain AS R1&#xA;&#x9;&#x9;WHERE &#xA;&#x9;&#x9;&#x9;R1.TxID = ft.FinancialTransactionId&#xA;&#x9;) AS R2&#xA;&#x9;OUTER APPLY&#xA;&#x9;(&#xA;&#x9;&#x9;SELECT&#xA;&#x9;&#x9;&#x9;OrganizationUnitId&#xA;&#x9;&#x9;FROM [dbo].[OrganizationUnitUser] As ouu&#xA;&#x9;&#x9;WHERE&#xA;&#x9;&#x9;&#x9;ouu.UserId = ft.UserId&#xA;&#x9;) AS orgUnit&#xA;&#x9;WHERE&#xA;&#x9;&#x9;COALESCE(ft.userID,-1) = driver.UserID&#xA;&#x9;&#x9;AND ft.FinancialTransactionTypeId = driver.FinancialTransactionTypeId&#xA;) By_User;&#xA;&#xA;CREATE UNIQUE CLUSTERED INDEX ix_Header ON ##BaseQuery&#xA;(SourceKey);" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ParameterBinding
