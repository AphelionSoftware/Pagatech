                  name="OLEDB_FinancialTransactionItems"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
	current_batch.*
FROM
(
	SELECT 
		FinancialTransactionItemID,
		FinancialAccountID = COALESCE(FinancialAccountId, -1)
	FROM dbo.FinancialTransactionItem AS x
                   WHERE x.financialTransactionItemId &gt; ?
	ORDER BY x.FinancialTransactionItemId
	OFFSET ? ROWS
	FETCH NEXT 100000 ROWS ONLY
) AS driver
CROSS APPLY
(

	SELECT	
		SourceKey = base_query.SourceKey,
		change_operation = 'I',
		base_query.DimCreatedDateID,
		base_query.DimCreatedTimeID,
		base_query.DimFinancialAccountSourceKey,
		base_query.DimPagaAccountSourceKey,
		base_query.FactFinancialTxHeaderSourceKey,
		base_query.TextDescription,
		base_query.CreditAmount,
		base_query.DebitAmount
	FROM
	(
		SELECT
			SourceKey = fti.FinancialTransactionItemId, 
			DimCreatedDateID = CONVERT(INT,CONVERT(VARCHAR(8),fti.CreatedDate,112)), 
			DimCreatedTimeID = CONVERT(INT,REPLACE(CONVERT(VARCHAR(8), fti.CreatedDate, 114),':','')), 
			DimFinancialAccountSourceKey = COALESCE(fti.FinancialAccountId, -1),
			DimPagaAccountSourceKey = COALESCE(paga_acct.PagaAccountId, -1),
			FactFinancialTxHeaderSourceKey =  fti.financialTransactionID,
			TextDescription = CONVERT(VARCHAR(200),fti.[Description]), 
			fti.CreditAmount, 
			fti.DebitAmount
		FROM  [dbo].[FinancialTransactionItem] AS fti
		LEFT JOIN 
		(
			SELECT 
				pa.PagaAccountId,
				fa.FinancialAccountId
			FROM dbo.pagaAccount AS pa 
			INNER JOIN dbo.FinancialAccount as fa ON
				pa.AccountHolderId = fa.AccountHolderId 
		) AS paga_acct ON
			paga_acct.FinancialAccountId = fti.FinancialAccountId
		WHERE 
			fti.financialTransactionItemID = driver.FinancialTransactionItemId
			AND COALESCE(fti.FinancialAccountId, -1) = driver.FinancialAccountId
	) AS base_query
) AS current_batch</property>
