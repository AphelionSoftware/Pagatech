    <DTS:Variable DTS:CreationName="" DTS:DTSID="{6F59B746-9F91-40B1-AB76-419590759FB3}" DTS:IncludeInDebugDump="2345" DTS:Namespace="User" DTS:ObjectName="Stream0">
      <DTS:VariableValue DTS:DataType="8">--PagaAccount
		SET NOCOUNT ON;
			
		DECLARE @grp AS TINYINT = 0

		SELECT
			stream.*,
			c.SYS_CHANGE_VERSION
		FROM
		(
			SELECT
				bq.*,
				g.grp
			FROM
			( 
				SELECT 
					as_of_change_version = CHANGE_TRACKING_CURRENT_VERSION(),
					PagaAccountId,
					ROW_NUMBER() OVER (ORDER BY PagaAccountId) as rn
				FROM [dbo].[PagaAccount]  AS bq
			) AS bq
			CROSS APPLY (SELECT (CAST(bq.rn AS VARCHAR) % 4) AS grp) as g
			WHERE 
					g.grp = @grp
		) AS stream
		CROSS APPLY 
			CHANGETABLE (VERSION [PagaAccount], (PagaAccountId), (stream.PagaAccountId)) AS c
		OPTION (MAXDOP 1)</DTS:VariableValue>
    </DTS:Variable>
