<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>Paga_EDW</TargetDatabaseName>
    <DeployScriptFileName>Paga_EDW.sql</DeployScriptFileName>
    <ScriptDatabaseOptions>True</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>False</BlockOnPossibleDataLoss>
    <CreateNewDatabase>True</CreateNewDatabase>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <TargetConnectionString>Data Source=.\SQL2012;Integrated Security=True;Pooling=False</TargetConnectionString>
    <IgnoreRoleMembership>True</IgnoreRoleMembership>
    <ScriptDatabaseCollation>True</ScriptDatabaseCollation>
    <ScriptNewConstraintValidation>False</ScriptNewConstraintValidation>
  </PropertyGroup>
</Project>